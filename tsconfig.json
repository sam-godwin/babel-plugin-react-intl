{
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "baseUrl": ".",
    "sourceMap": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ESNext",
    "lib": [
      "dom",
      "esnext.intl",
      "es2017.intl",
      "es2018.intl",
      "ES2015"
    ],
    "declaration": true,
    "strict": true,
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true,
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "declarationMap": true,
    "jsx": "react",
    "importHelpers": true,
  
